<h1>Listing array_currents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @array_currents.each do |array_current| %>
      <tr>
        <td><%= link_to 'Show', array_current %></td>
        <td><%= link_to 'Edit', edit_array_current_path(array_current) %></td>
        <td><%= link_to 'Destroy', array_current, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Array current', new_array_current_path %>
