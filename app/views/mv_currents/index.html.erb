<h1>Listing mv_currents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mv_currents.each do |mv_current| %>
      <tr>
        <td><%= link_to 'Show', mv_current %></td>
        <td><%= link_to 'Edit', edit_mv_current_path(mv_current) %></td>
        <td><%= link_to 'Destroy', mv_current, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mv current', new_mv_current_path %>
